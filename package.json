{
  "name": "@tmnrp/react-npm-library-boiler-plate",
  "version": "0.0.1",
  "description": "Boiler plate to create npm package for react components",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack --mode production",
    "test": "jest"
  },
  "repository": "https://github.com/tmnrp/packages/tree/master/lib/reactjs-range-slider",
  "keywords": [],
  "author": "Parammal Harsh (tmnrp)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tmnrp/packages/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tmnrp/packages/tree/master/lib/reactjs-range-slider#readme",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "lib/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^25.5.4",
    "node-sass": "^4.14.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
